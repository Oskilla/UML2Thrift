-- @nsURI UML=http://www.eclipse.org/uml2/5.0.0/UML
-- @path IDL=/Thrift/src/main/ecore/thrift-idl.ecore
-- Transforms UML Interfaces into Thrift services

module uml2thrift;
create OUT : IDL from IN : UML;

rule interface2service {
	from
		i : UML!Interface
	to
		s : IDL!Service (
				name <- i.name
			)
		
} 